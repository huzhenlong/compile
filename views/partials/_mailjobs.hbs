
<div class="ibox float-e-margins" ng-app="task" ng-controller="taskList">
    <div class="ibox-title">
        <h5>{[{taskList}]}</h5>
    </div>
    <div class="ibox-content">
        <div class="row">
            <div class="col-md-12" style="margin:0 0 10px 15px;display: none;">
                <div class="row">
                    <form role="form" class="form-inline" id="form_search"
                          onkeydown="if(event.keyCode==13){return false;}">

                        <div class="form-group">
                            <label class="labelBlock">{[{taskName}]}：</label>
                            <input type="text" placeholder="{[{importTaskName}]}" name="campaignTaskName"
                                   class="form-control">
                        </div>

                        <div class="form-group">
                            <label class="labelBlock">{[{sendTime}]}：</label>
                            <input type="date" placeholder="{[{importStartTime}]}" name="startTime"
                                   class="form-control">-
                            <input type="date" placeholder="{[{importEndTime}]}" name="endTime"
                                   class="form-control">
                        </div>

                        <button class="btn btn-info" type="button" id="checkData" title="{[{search}]}" style="margin-bottom: 0;"><i
                                class="fa fa-search"></i>
                        </button>
                    </form>
                </div>
            </div>
        </div>
        <!--会员列表-->
        <table class="table table-striped table-bordered table-hover " id="job_table">
        </table>
    </div>
</div>


<script src="/js/angularJs/_mailjobs.js"></script>




